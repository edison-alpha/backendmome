# Server
PORT=3000
NODE_ENV=development

# Redis (Redis Labs)
REDIS_HOST=your-redis-host.redislabs.com
REDIS_PORT=12345
REDIS_USERNAME=default
REDIS_PASSWORD=your_redis_password_here

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Movement Network
MOVEMENT_INDEXER_URL=https://indexer.testnet.movementnetwork.xyz/v1/graphql
RAFFLE_CONTRACT_ADDRESS=your_contract_address_here

# CORS
ALLOWED_ORIGINS=http://localhost:8080,https://your-frontend.com

# Cache TTL (seconds)
CACHE_TTL_SHORT=30
CACHE_TTL_MEDIUM=300
CACHE_TTL_LONG=3600

# Features
ENABLE_ANALYTICS=true
ENABLE_SUPABASE=true

# Security (for IP hashing)
IP_SALT=your_random_salt_here

# Pinata IPFS (SECURE - only on backend)
PINATA_JWT=your_pinata_jwt_here
PINATA_API_KEY=your_pinata_api_key_here
PINATA_SECRET_KEY=your_pinata_secret_key_here
PINATA_GATEWAY=https://gateway.pinata.cloud/ipfs
